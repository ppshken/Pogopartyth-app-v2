{
  "expo": {
    "name": "PogopartyTH",
    "slug": "pogopartyth-app",
    "version": "1.0.0",
    "icon": "./assets/pogopartyth.png",
    "userInterfaceStyle": "light",
    "scheme": "pogopartyth",
    "splash": {
      "image": "./assets/pogopartyth.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "translucent": false,
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      
      "expo-notifications",
      ["@react-native-google-signin/google-signin", {
        "iosUrlScheme": "com.googleusercontent.apps.926863512286-c2s6shb73ot66n55md241oqt0btslqko"
      }],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-router",
      "expo-font"
    ],
    "ios": {
      "bundleIdentifier": "com.ppshken.pogopartyth",
      "buildNumber": "1"
    },
    "android": {
      "package": "com.ppshken.pogopartyth",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "pogopartyth"
            }
          ]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "e6edfdf9-73e9-4a17-a5dc-efec2368ff56"
      }
    }
  }
}
